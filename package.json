{
  "name": "loopback4",
  "version": "0.0.1",
  "description": "loopback4",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t loopback4 .",
    "docker:run": "docker run -p 3000:3000 -d loopback4",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "dev": "nodemon",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "_moduleAliases": {
    "$config": "dist/config",
    "$datasources": "dist/datasources",
    "$events": "dist/events",
    "$models": "dist/models",
    "$plugins": "dist/plugins",
    "$repositories": "dist/repositories",
    "$services": "dist/services"
  },
  "dependencies": {
    "@loopback/boot": "^5.0.2",
    "@loopback/core": "^4.0.2",
    "@loopback/cron": "^0.9.3",
    "@loopback/repository": "^5.0.2",
    "@loopback/rest": "^12.0.2",
    "@loopback/rest-explorer": "^5.0.2",
    "@loopback/service-proxy": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mysql": "^5.4.4",
    "loopback-connector-postgresql": "^5.5.1",
    "loopback-connector-rest": "^4.0.1",
    "module-alias": "^2.2.2",
    "triple-beam": "^1.3.0",
    "tslib": "^2.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.2",
    "@loopback/eslint-config": "^13.0.2",
    "@loopback/testlab": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^14.18.21",
    "@types/triple-beam": "^1.3.2",
    "@types/winston": "^2.4.4",
    "eslint": "^8.19.0",
    "nodemon": "^2.0.19",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.7.4"
  }
}
